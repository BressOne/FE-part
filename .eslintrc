{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "react/prop-types": [2],
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "extends": ["airbnb-base"]
}
